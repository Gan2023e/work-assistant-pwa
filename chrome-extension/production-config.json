{
  "environment": "production",
  "railway": {
    "domain": "work-assistant-pwa-production.up.railway.app",
    "api_base_url": "https://work-assistant-pwa-production.up.railway.app",
    "protocol": "https"
  },
  "netlify": {
    "frontend_url": "https://your-netlify-app.netlify.app",
