  "chrome_extension": {
    "host_permissions": [
      "http://localhost:*/*",
      "https://*/*",
      "https://work-assistant-pwa-production.up.railway.app/*"
    ],
    "content_script_matches": [
      "http://localhost:*/*",
      "https://*/*",
      "https://work-assistant-pwa-production.up.railway.app/*"
    ]
  }
} 